html, body{
  width: 100%;
  height: 100%;
}
#app{
  width: 100%;
  height: 100%;
}
.start-box {
  display: box;
  display: -webkit-box;
}
.box-orient {
  box-orient: vertical;
  -webkit-box-orient: vertical;
}
.box-align {
  box-align:center;
  -webkit-box-align:center;
}
.box-pack {
  box-pack:justify;
  -webkit-box-pack:justify;
}
.pack-center{
  box-pack:center;
  -webkit-box-pack:center;
}
.box-flex{
  box-flex:1;
  -webkit-box-flex:1;
}
.box-flex19{
  box-flex:19;
  -webkit-box-flex:19;
}
.box-flex5{
  box-flex:5;
  -webkit-box-flex:5;
}
.app-main {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 99;
}
.setHeight {
  height: -webkit-calc(100% - 60px);
  height: calc(100% - 60px);
}
.content-padding {
  height: 100%;
  padding:20px;
}
.content-padding .ivu-table-body {
  height: calc(100% - 40px);
  height: -webkit-calc(100% - 40px);
}
.add-video-bt {
  width: 200px;
  height: 50px;
  margin-left: 90px;
  border: 2px dashed #e9eaec;
  text-align: center;
  cursor: pointer;
  margin-top: 20px;
}
.cursor {
  cursor: pointer;
}
.app-main2 {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 99;
  overflow: auto;
  background: #F5F7F9;
}
/*body {
  overflow: hidden;
}*/
.line {
  width: 100%;
  height: 1px;
  margin:20px auto;
  margin-top: 60px;
  background: #dddee1;
}
/* .layout-content{
  min-height: 600px !important;
} */
.webuploader-pick {
  /*position: absolute;*/
  width: 100%;
  height: 100%;
  display: box;
  display: -webkit-box;
  box-align:center;
  -webkit-box-align:center;
  box-pack:center;
  -webkit-box-pack:center;
}



