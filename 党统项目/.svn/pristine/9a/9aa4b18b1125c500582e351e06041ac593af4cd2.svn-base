<template>
<div class="memberinfo-representative">
    <div class="text-right">
        <Button type="primary" @click="add">新增</Button>
    </div>
    <Table ref="memberinfoposttable" :highlight-row="true" :style="{'marginTop':'8px'}" border :columns="tableCol" :data="lists"></Table>
    <component v-bind:is="modal"></component>
</div>
</template>
<script>
import add from '../pop/partymember/memberinfo-representative-add.vue';
export default {
    name : 'memberinfo-representative',
    props: {
        viewdata : null
    },
    components: {
        'memberinfo-representative-add': add
    },
    methods:{
        add () {
            // this.$router.push('/dangyuan/modal/representative-add');
            this.$router.push({ 
                name: 'dangyuaninfo', 
                params: { 
                    uid: this.$route.params.uid
                },
                query: { 
                    tab: this.$route.query.tab,
                    model: 'memberinfo-representative-add'
                }
            });
        },
    },
    data(){
        return {
            tableCol :[{
                title : '党代会级别',
                key : 'start'      
            },{
                title : '开始时间',
                key : 'end'      
            },{
                title : '结束时间',
                key : 'name'      
            },{
                title : '党代表类型',
                key : 'name'      
            },{
                title : '操作',
                key : 'opt'
            }],
            lists:[],
            modal : null
        }
    },
    mounted(){
        if (this.$route.query.model) {
            this.modal = this.$route.query.model
        };
    },
    watch: {
        '$route' (to, from) {
            console.log(to);
            if (to.query.model) {
                this.modal = to.query.model
            }
            else {
                this.modal = null;
            }
        }
    }
}
</script>
<style scoped>
.memberinfo-representative{
    height: 100%;
    padding: 8px;
}
</style>