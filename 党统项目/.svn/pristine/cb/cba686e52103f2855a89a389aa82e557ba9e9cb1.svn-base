.start-box {
	display: box;
	display: -webkit-box;
}
.box-orient {
	box-orient: vertical;
	-webkit-box-orient: vertical;
}
.box-align {
	box-align:center;
	-webkit-box-align:center;
}
.box-pack {
	box-pack:justify;
	-webkit-box-pack:justify;
}
.pack-center{
  box-pack:center;
  -webkit-box-pack:center;
}
.box-flex{
  box-flex:1;
  -webkit-box-flex:1;
}
.box-flex19{
  box-flex:19;
  -webkit-box-flex:19;
}
.box-flex5{
  box-flex:5;
  -webkit-box-flex:5;
}
.app-main {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 99;
}
.setHeight {
	height: -webkit-calc(100% - 60px);
	height: calc(100% - 60px);
}
.content-padding {
	padding:20px;
}
.add-video-bt {
  width: 200px;
  height: 50px;
  margin-left: 90px;
  border: 2px dashed #e9eaec;
  text-align: center;
  cursor: pointer;
  margin-top: 20px;
}
.cursor {
  cursor: pointer;
}
.app-main2 {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 99;
  overflow: auto;
  background: #F5F7F9;
}
body {
  overflow: hidden;
}
.line {
  width: 100%;
  height: 1px;
  margin:20px auto;
  margin-top: 60px;
  background: #dddee1;
}
.layout-content{
  min-height: 600px !important;
}
.webuploader-pick {
  /*position: absolute;*/
  width: 100%;
  height: 100%;
  display: box;
  display: -webkit-box;
  box-align:center;
  -webkit-box-align:center;
  box-pack:center;
  -webkit-box-pack:center;
}



/*党统*/
.layout-menu-left {
  background: #fff !important;
}
.top-bar {
  height: 100%;
  background: #fff;
  border-bottom: 1px solid #ddd;
  padding-right: 20px;
}
.el-tree {
  height: 100%;
}
.info-padding {
  padding: 0 10px;
}
.cursor-point {
  cursor: pointer;
}
.info-content {
  background: #fff;
  width: 55%;
  height: 100%;
  position:absolute; right: 0;top:0;z-index: 50;
  box-shadow: -5px 0 4px #ddd;
}
.info-title {
  height: 40px;
  background: #f0f0f0;
  border-bottom: 1px solid #ddd;
}
.table-switch {
  padding: 10px 10px 5px 10px;
  border-bottom: 1px solid #ddd;
}
.info-footer {
  height: 60px;
  background: #f0f0f0;
  border-top: 1px solid #ddd;
  padding: 0 10px;
}
.info-detail {
  padding: 10px;
  padding-left: 0px;
  overflow: auto;
}
.nav-manage {
  font-size: 14px;
  color: #333;
}
.nav-manage a {
  color: #333;
}
.tree-icons {
  display: inline-block;
  margin-right: 3px;
}
.tree-icons-1 {
  width: 10px;
  height: 12px;
  background: url(../assets/types/1.png) no-repeat center bottom;
}
.tree-icons-2 {
  width: 12px;
  height: 11px;
  background: url(../assets/types/2.png) no-repeat center bottom;
}
.tree-icons-3 {
  width: 12px;
  height: 12px;
  background: url(../assets/types/3.png) no-repeat center bottom;
}
.tree-icons-4 {
  width: 12px;
  height: 11px;
  background: url(../assets/types/4.png) no-repeat center bottom;
}
.tree-icons-5 {
  width: 12px;
  height: 12px;
  background: url(../assets/types/5.png) no-repeat center bottom;
}
.tree-icons-6 {
  width: 12px;
  height: 13px;
  background: url(../assets/types/6.png) no-repeat center bottom;
}
